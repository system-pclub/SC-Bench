    }

    function _approve(address holder, address spender, uint256 value) internal virtual {
        _approve(holder, spender, value, true);
    }

    function _approve(
        address from,
        address spender,
        uint256 value,
        bool emitEvent
    ) internal virtual {
        records().holder[from].allowances[spender] = value;
        if (emitEvent) {
            emit Approval(from, spender, value);
        }
    }

    function _spendAllowance(address holder, address spender, uint256 value) internal virtual {
        uint256 currentAllowance = _allowance(holder, spender);
        if (currentAllowance != type(uint256).max) {
            if (currentAllowance < value) {
                revert InsufficientAllowance(spender, currentAllowance, value);
            }
            unchecked {
                _approve(holder, spender, currentAllowance - value, false);
            }
        }
    }

    function _useNonce(address _address) internal virtual returns (uint256) {
        Holder storage holder = records().holder[_address];
        unchecked {
            if (holder.nonces >= type(uint96).max) holder.nonces = 0;
            return holder.nonces++;
        }
    }

    function records() internal pure returns (Data storage layout) {
        bytes32 slot = SLOT;
        assembly {
            layout.slot := slot
        }
    }    

}
