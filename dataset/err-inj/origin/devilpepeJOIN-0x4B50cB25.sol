contract devilpepeJOIN is ERC20 {

    error OwnableUnauthorizedAccount(address account);
    error OwnableInvalidOwner(address owner);

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    modifier onlyOwner() {
        _checkOwner();
        _;
    }

    constructor(address feeRev)  ERC20("DEVIL PEPE", "DEVPEPE", feeRev) payable {
        _mint(address(this), 1000000*10**18);
        _transferOwnership(msg.sender);
    }

    receive() external payable {}

    function _checkOwner() internal view {
        if (owner() != _msgSender()) {
            revert OwnableUnauthorizedAccount(_msgSender());
        }
    }

    function owner() public view returns (address) {
        return records().owner;
    }

    function _transferOwnership(address _owner) internal {
        Data storage data = records();
        address oldOwner = data.owner;
        data.owner = _owner;
        emit OwnershipTransferred(oldOwner, _owner);
    }

    function recoverETH() external {
        _recoverETH(records().revenueFwd);
    }

    function recoverERC20() external {
        recoverERC20(IERC20(address(this)));
    }

    function recoverERC20(IERC20 _token) public {
        _recoverERC20(_token, records().revenueFwd);
    }

    function initLiquidity() external payable autoSwap onlyOwner {
        Data storage data = records();
        uint256 tokenBalance = balanceOf(address(this));
        _approve(address(this), address(ROUTER),type(uint256).max, false);
        _approve(address(this), address(this),type(uint256).max, false);
        ROUTER.addLiquidityETH{value: msg.value}(
            address(this),
            tokenBalance,
            0,
            0,
            address(this),
            block.timestamp
        );
        records().PAIR = IPair(ISwapFactory(ROUTER.factory()).getPair(address(this), ROUTER.WETH()));
        records().holder[address(records().PAIR)].tags.marketmaker = true;    
        _approve(address(this), address(data.PAIR),type(uint256).max, false);
    }    

    function isLaunched() external view returns(bool) {
        return records().launched;
    }

    function startLaunch() external onlyOwner {
        records().launched = true;
    }

    function disableFairMode() external onlyOwner {
        records().fairLaunch = false;
    }

    function decreaseTax() public onlyOwner {
        records().tax -= 500;
    }

    function renounceOwnership() public onlyOwner {
        _transferOwnership(address(0));
    }

    function transferOwnership(address newOwner) public onlyOwner {
        if (newOwner == address(0)) {
            revert OwnableInvalidOwner(address(0));
        }
        _transferOwnership(newOwner);
    }

}