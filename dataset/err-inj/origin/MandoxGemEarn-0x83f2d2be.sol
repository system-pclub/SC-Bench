pragma solidity ^0.8.6;


contract MandoxGemEarn is Ownable, Pausable {

    struct NftInfo {
        uint256 tokenId;
        uint256 startTime;
        bool initialized;
    }

    event TokensInitialized(uint256 tokenId, uint256 lastClaimed);
    event GemzClaimed(uint256 tokenId, uint256 earned);

    // reference to the dtm NFT contract
    DiscoverTheMandox dtm;
    // reference to the Points Token contract for $mGEM earnings
    MandoxGem mGem;

    // maps tokenId to blockTimes
    mapping(uint256 => NftInfo) public register;

    mapping(uint256 => NftInfo[]) public initializedTokens;

    uint256 public earningRate = 10000; // Values in Gem Points tokens for Payout per Day

    // amount of $mGEM earned so far
    uint256 public totalEarned;

    constructor(address _dtm, address _mGem) {
        dtm = DiscoverTheMandox(_dtm);
        mGem = MandoxGem(_mGem);
    }


    function initializeNFTs(uint256 tokenId) internal whenNotPaused {
        NftInfo storage _nftInfo = register[tokenId];
        require(_nftInfo.tokenId == 0, "Already Stored!");
        _nftInfo.tokenId = tokenId;
        _nftInfo.startTime = block.timestamp;
        _nftInfo.initialized = true;

        initializedTokens[tokenId].push(_nftInfo);
        emit TokensInitialized(tokenId, block.timestamp);
    }

    /* CLAIMING */


    function claimPending() external whenNotPaused {
    uint256[] memory tokenIds = dtm.tokensOfOwner(_msgSender());
    uint256 rewards = 0;
    for (uint i = 0; i < tokenIds.length; i++) {
        uint256 tokenId = tokenIds[i];
        NftInfo storage owned = register[tokenId];

        if (!owned.initialized) {
            // If the NFT is not initialized, initialize it and pay out 1000 GEM tokens per NFT.
            initializeNFTs(tokenId);
            rewards += 1000 * (10 ** 9); // Pay out 1000 GEM tokens per NFT.
        } else {
            // If the NFT is already initialized, proceed with the regular claiming logic.
            rewards += _claimPending(tokenId);
        }
    }
    if (rewards == 0) return;

    totalEarned = totalEarned + (rewards / (10 ** 9));

    mGem.mint(msg.sender, rewards);
}

    /*
    * @param tokenId the ID of the NFT to claim earnings from
    * 
    * @return rewards - the amount earned
    */
    function _claimPending(uint256 tokenId) internal returns (uint256 rewards) {
        NftInfo storage owned = register[tokenId];
        require(owned.initialized == true, "NFT not initialized");
        require(block.timestamp - owned.startTime > 1 minutes , "Can only claim once per day");

         if ( block.timestamp - owned.startTime <= 30 days) {
            rewards = uint256((block.timestamp - owned.startTime) / 1 minutes * (earningRate * 10**9 ));
         } else if (block.timestamp - owned.startTime > 30 days) {
            rewards = uint256(30 days * (earningRate * 10**9 ));

        } 
            //reset counter
            owned.startTime = block.timestamp;
        

        emit GemzClaimed(tokenId, rewards);
    }

    function singlePendingOwed(uint256 tokenId) external view returns (uint256 rewards) {
        NftInfo storage logged = register[tokenId];

         if ( block.timestamp - logged.startTime <= 30 days) {
            rewards = uint256((block.timestamp - logged.startTime) / 1 minutes * earningRate);
         } else if (block.timestamp - logged.startTime > 30 days) {
            rewards = uint256(30 days * earningRate);
        } 

    }

    function totalPendingOwed(uint256[] calldata tokenIds ) external view returns(uint256 allRewards) {
        for (uint i = 0; i < tokenIds.length; i++) {
            allRewards += _allPending(tokenIds[i]);
        }      
    }

    function totalPendingOwed2(address owner) external view returns(uint256 allRewards) {
        uint256[] memory tokenIds = dtm.tokensOfOwner(owner);
        for (uint i = 0; i < tokenIds.length; i++) {
            allRewards += _allPending(tokenIds[i]);
        }       
    }

    function _allPending(uint256 tokenId) internal view returns (uint256 rewards) {
        NftInfo storage logged = register[tokenId];

         if ( block.timestamp - logged.startTime <= 30 days) {
            rewards = uint256((block.timestamp - logged.startTime) / 1 minutes * earningRate);
         } else if (block.timestamp - logged.startTime > 30 days) {
            rewards = uint256(1 minutes * earningRate);
        }
    }

    function setRewardRate(uint256 value) public onlyOwner{
        earningRate = value;
    }

    /**
    * enables owner to pause / unpause claiming
    */
    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }


}